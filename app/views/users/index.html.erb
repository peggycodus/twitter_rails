<h1>Users Listing</h1>

<table>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <tr><td><%=h user.name %></td><td><%=h user.email %></td><td><%= link_to "Follow", followings_path(:followed_id => user), :method => :post %></td></tr>
      </tr>
    <% end %>
    <%= paginate @users %>
  </tbody>
</table>
